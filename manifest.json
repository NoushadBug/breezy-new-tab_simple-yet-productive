{
    "manifest_version": 3,
    "name": "Breeze New Tab",
    "version": "1.0",
    "description": "A minimalistic and awesome styled new tab page.",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "icons/*"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "icons/icon_png.png",
        "48": "icons/icon_png.png",
        "128": "icons/icon_png.png"
    }
}